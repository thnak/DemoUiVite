  "scripts": {
    "dev": "npm run generate:api && vite",
    "start": "vite preview",
    "build": "npm run generate:api && tsc && vite build",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "fm:fix": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "fix:all": "npm run lint:fix && npm run fm:fix",
    "clean": "rm -rf node_modules .next out dist build",
    "re:dev": "npm run clean && npm install && npm run dev",
    "re:build": "npm run clean && npm install && npm run build",
    "tsc:dev": "npm run dev & npm run tsc:watch",
    "tsc:watch": "tsc --noEmit --watch",
    "generate:api": "tsx scripts/generate-api.ts",
    "generate:selectors": "tsx scripts/generate-entity-selectors.ts"
  },
